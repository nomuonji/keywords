{
  "name": "keywords-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch --tsconfig tsconfig.server.json server/main.ts",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "preview": "vite preview",
    "start": "node dist/server/main.js"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.9.0",
    "@google/generative-ai": "^0.24.1",
    "@tavily/core": "^0.5.12",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.3.6",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.1.0",
    "groq-sdk": "^0.35.0",
    "node-fetch": "^3.3.2",
    "pino": "^8.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/yargs": "^17.0.33",
    "@vercel/node": "^3.2.10",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.0",
    "vite": "^5.2.0"
  }
}
